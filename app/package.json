{
  "name": "myplacesapi",
  "version": "0.0.0",
  "private": true,
  "files": [
    "bin"
  ],
  "scripts": {
    "start": "node ./bin/www",
    "transpile": "babel --presets es2015 ./server.js ./models/* ./utils/* ./api/* --out-dir bin",
    "create": "npm run transpile && claudia create --region us-east-1 --api-module bin/api",
    "update": "npm run transpile && claudia update"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "babel-cli": "^6.26.0",
    "babel-preset-es2017": "^6.24.1",
    "bcrypt": "^2.0.1",
    "body-parser": "^1.18.2",
    "busy-hours": "0.0.3",
    "claudia-api-builder": "^4.0.0",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "express-session": "^1.15.6",
    "http-errors": "~1.6.2",
    "jade": "~1.11.0",
    "mongoose": "^5.0.15",
    "morgan": "~1.9.0",
    "regenerator-runtime": "^0.11.1",
    "socket.io": "^2.1.0"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.24.1",
    "claudia": "^4.0.0"
  }
}
