    "marked": "^0.3.19",
    "mongoose": "^5.0.12",
    "nodemon": "^1.17.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-draggable": "^3.0.5",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
